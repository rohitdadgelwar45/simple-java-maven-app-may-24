pipeline {
    agent any

    tools{
        maven "mvn"
    }

    stages {
        stage('build') {
            steps {
                echo 'build'
            }
        }

       
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deliver') {
            steps {
                echo 'Deploy on Ec-2'
            }
        }
    }
}
